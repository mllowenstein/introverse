{
  "name": "introverse",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "app:scan": "npx prettier --check .",
    "app:format": "npx prettier --write .",
    "server": "json-server db.json --delay 500",
    "app:prettify": "prettier --write \"src/**/*.ts\"",
    "prettier": "prettier --config .prettierrc --write .",
    "build:prod": "ng build --configuration production",
    "local": "ng serve --configuration local --port 4211",
    "remote": "ng serve --configuration local --host 0.0.0.0",
    "watch": "ng build --watch --configuration development",
    "build:analyze": "ng build --configuration production --source-map",
    "explorer": "npm run build:analyze && source-map-explorer dist/**/*.js",
    "docs:build-and-serve": "compodoc -p tsconfig.doc.json -s",
    "docs:build": "compodoc -p tsconfig.doc.json",
    "docs:serve": "compodoc -s",
    "cy:run:ci": "cypress run --record --key f64482a3-d127-4cba-a56e-a8ca0e6cef7e",
    "cy:open:dev": "cypress open --config baseUrl=https://localhost:4211",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "format": "npx prettier --write .",
    "scan": "npx prettier --check .",
    "build:ci": "npm run build",
    "generate": "ng generate"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/cdk": "^16.2.12",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/localize": "^16.2.12",
    "@angular/material": "^16.2.12",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@apollo/client": "^3.8.10",
    "@ngneat/until-destroy": "^10.0.0",
    "@ngx-translate/core": "^15.0.0",
    "ag-grid-angular": "^31.0.1",
    "ag-grid-community": "^31.0.1",
    "apollo-angular": "^6.0.0",
    "d3": "^7.8.5",
    "ether.js": "^1.0.0",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "hammerjs": "^2.0.8",
    "highcharts": "^11.2.0",
    "highcharts-angular": "^4.0.0",
    "moment": "^2.30.1",
    "primeicons": "^6.0.1",
    "primeng": "^16.9.1",
    "rxjs": "~7.8.0",
    "three": "^0.160.0",
    "tslib": "^2.3.0",
    "web3": "^4.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^17.0.0",
    "@angular-devkit/build-angular": "^16.2.11",
    "@angular-eslint/builder": "^16.3.1",
    "@angular-eslint/eslint-plugin": "^16.3.1",
    "@angular-eslint/eslint-plugin-template": "^16.3.1",
    "@angular-eslint/schematics": "^16.3.1",
    "@angular-eslint/template-parser": "^16.3.1",
    "@angular/cli": "^16.2.11",
    "@angular/compiler-cli": "^16.2.0",
    "@compodoc/compodoc": "^1.1.23",
    "@types/d3": "^7.4.3",
    "@types/jasmine": "~4.3.0",
    "@types/three": "^0.160.0",
    "@types/web3": "^1.2.2",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "jasmine-core": "~4.6.0",
    "json-server": "^1.0.0-alpha.13",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.1.1",
    "source-map-explorer": "^2.5.3",
    "typescript": "~5.1.3"
  }
}
